{
	"env": {
		"es6": true,
		"browser": true
	},
	"extends": "airbnb-base",
	"rules": {
		"import/extensions": 0,
		"import/no-unresolved": 0,
		"import/prefer-default-export": 0,
		"max-len": 0,
		"no-bitwise": 0,
		"no-param-reassign": 0,
		"semi": 0
	},
	"globals": {
		"BAEP": true
	},
	"parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    }
}